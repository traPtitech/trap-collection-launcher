{
  "name": "trap-collection",
  "productName": "trap-collection",
  "version": "0.0.5",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "dotenv electron-forge start",
    "package": "dotenv electron-forge package",
    "make": "dotenv electron-forge make",
    "publish": "dotenv electron-forge publish",
    "lint": "eslint --ext .ts .",
    "postinstall": "husky install",
    "open-content-directory-for-windows": "start %APPDATA%/trap-collection/contents",
    "api:build": "openapi-generator-cli generate -g typescript-axios -i ./docs/openapi.yml -o ./src/lib/typescript-axios",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [],
  "author": {
    "name": "traP"
  },
  "license": "MIT",
  "config": {
    "forge": "./forge.config.js"
  },
  "lint-staged": {
    "*.{md,html,css}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@types/react-transition-group": "^4.4.4",
    "axios": "^0.24.0",
    "cleave.js": "^1.6.0",
    "electron-log": "^4.4.3",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-spinners": "^0.11.0",
    "styled-components": "^5.3.3",
    "update-electron-app": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@electron-forge/plugin-webpack": "6.0.0-beta.61",
    "@electron-forge/publisher-github": "^6.0.0-beta.61",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@openapitools/openapi-generator-cli": "^2.4.21",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "@types/adm-zip": "^0.4.34",
    "@types/cleave.js": "^1.4.6",
    "@types/electron-store": "^3.2.0",
    "@types/iconv-lite": "^0.0.1",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-spinner": "^0.2.0",
    "@types/styled-components": "^5.1.19",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "adm-zip": "^0.5.9",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "css-loader": "^6.5.1",
    "dotenv-cli": "^4.1.1",
    "electron": "16.0.5",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.5",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "husky": "^7.0.4",
    "iconv-lite": "^0.6.3",
    "lint-staged": "^12.1.4",
    "node-loader": "^2.0.0",
    "prettier": "^2.5.1",
    "react-transition-group": "^4.4.2",
    "storybook-addon-styled-component-theme": "^2.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "4.5.4"
  }
