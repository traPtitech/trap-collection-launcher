{
  "name": "trap-collection",
  "productName": "trap-collection",
  "version": "2.5.0",
  "description": "traP 成果物ランチャー",
  "main": ".vite/build/main.cjs",
  "type": "module",
  "scripts": {
    "start": "dotenv electron-forge start",
    "package": "dotenv electron-forge package",
    "make": "dotenv electron-forge make",
    "publish": "dotenv electron-forge publish",
    "lint": "eslint --ext .ts .",
    "postinstall": "husky install",
    "open-content-directory-for-windows": "start %APPDATA%/trap-collection/contents",
    "api:build": "openapi-generator-cli generate -g typescript-axios -i ./docs/openapi.yml -o ./src/lib/typescript-axios",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "typecheck": "tsc -p . --noEmit"
  },
  "keywords": [],
  "author": {
    "name": "traP"
  },
  "license": "MIT",
  "config": {
    "forge": "./forge.config.js"
  },
  "lint-staged": {
    "*.{md,html,css}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@types/react-transition-group": "^4.4.12",
    "axios": "^1.12.2",
    "cleave.js": "^1.6.0",
    "electron-log": "^5.4.3",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^11.0.0",
    "memfs": "^4.48.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-spinners": "^0.17.0",
    "styled-components": "^6.1.19",
    "update-electron-app": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@electron-forge/cli": "^7.9.0",
    "@electron-forge/maker-squirrel": "^7.9.0",
    "@electron-forge/maker-zip": "^7.9.0",
    "@electron-forge/plugin-vite": "^7.9.0",
    "@electron-forge/publisher-github": "^7.9.0",
    "@openapitools/openapi-generator-cli": "^2.24.0",
    "@storybook/addon-actions": "^9.0.8",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-links": "^9.1.10",
    "@storybook/react": "^9.1.10",
    "@types/adm-zip": "^0.5.7",
    "@types/cleave.js": "^1.4.12",
    "@types/iconv-lite": "^0.0.1",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@types/react-spinner": "^0.2.4",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitejs/plugin-react": "^4.7.0",
    "adm-zip": "^0.5.16",
    "babel-plugin-module-resolver": "^5.0.2",
    "dotenv-cli": "^10.0.0",
    "electron": "^38.2.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.0",
    "eslint-plugin-storybook": "^9.1.10",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "iconv-lite": "^0.7.0",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "react-transition-group": "^4.4.5",
    "storybook-addon-styled-component-theme": "^2.0.0",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^5.4.20",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6"
  }
}
